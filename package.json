{
  "name": "general-client-node",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "test:ci": "jest --ci --runInBand",
    "snyk": "snyk",
    "snyk:test": "snyk test",
    "snyk:fix": "snyk fix",
    "sonarqube": "docker compose -f ./ops/sonarqube/docker-compose.sonarqube.yml up",
    "sonar-scanner": "sonar-scanner",
    "format": "prettier --write . --ignore-path .prettierignore",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx,.js,.jsx src/ --fix",
    "lint-folder": "eslint -c .eslintrc.json --fix --ext .ts,.tsx,.js,.jsx",
    "commitlint": "npx --no -- commitlint --edit $1",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "axios": "1.4.0",
    "axios-retry": "3.5.0",
    "classnames": "2.3.2",
    "inversify": "6.0.1",
    "next": "13.4.3",
    "platform": "1.3.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "redux": "4.2.1",
    "reflect-metadata": "0.1.13",
    "uuid": "9.0.0",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.2.1",
    "@types/platform": "1.3.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-plugin-boundaries": "3.1.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-jsdoc": "44.2.4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.2",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "snyk": "1.1159.0",
    "sonarqube-scanner": "3.0.1",
    "stylelint": "15.6.2",
    "stylelint-config-standard": "33.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
