{
  "name": "general-server-node",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "test:ci": "jest --ci --runInBand",
    "prisma:generate": "prisma generate --schema ./src/common/infrastructure/database/orm/prisma/schema.prisma",
    "prisma:create-schema": "sh ./create-schema.sh",
    "snyk": "snyk",
    "snyk:test": "snyk test",
    "snyk:fix": "snyk fix",
    "sonarqube": "docker compose -f ./ops/sonarqube/docker-compose.sonarqube.yml up",
    "sonar-scanner": "sonar-scanner",
    "format": "prettier --write . --ignore-path .prettierignore",
    "lint": "eslint -c .eslintrc.json --ext .ts,.js src/ --fix",
    "lint-folder": "eslint -c .eslintrc.json --fix --ext .ts,.js",
    "commitlint": "npx --no -- commitlint --edit $1",
    "lint-staged": "lint-staged"
  },
  "prisma": {
    "schema": "./src/common/infrastructure/database/orm/prisma/schema.prisma"
  },
  "dependencies": {
    "@godaddy/terminus": "4.12.0",
    "@prisma/client": "4.15.0",
    "@wufe/mapper": "0.5.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-fields": "2.0.3",
    "graphql-http": "1.18.0",
    "helmet": "7.0.0",
    "inversify": "6.0.1",
    "inversify-express-utils": "6.4.3",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "logform": "2.5.1",
    "method-override": "3.0.0",
    "overload-protection": "1.2.3",
    "prom-client": "14.2.0",
    "redis": "4.6.7",
    "reflect-metadata": "0.1.13",
    "response-time": "2.3.2",
    "uuid": "9.0.0",
    "winston": "3.9.0",
    "winston-daily-rotate-file": "4.7.1",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/graphql-fields": "1.3.5",
    "@types/jsonwebtoken": "9.0.2",
    "@types/lodash": "4.14.195",
    "@types/method-override": "0.0.32",
    "@types/node": "20.2.5",
    "@types/overload-protection": "1.2.1",
    "@types/response-time": "2.3.5",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.42.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-boundaries": "3.1.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-jsdoc": "46.2.0",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prisma": "4.15.0",
    "snyk": "1.1173.0",
    "sonarqube-scanner": "3.0.1",
    "swagger-ui-express": "4.6.3",
    "typescript": "5.0.4"
  }
}
